#include<iostream>
using namespace std;
void fun(int *a,int num,int grade)
{
	if(num>10)
	{
		if(grade==100)
		{
			for(int i=1;i<11;i++)cout<<a[i];
			cout<<endl;
		}
		return;
	}
	a[num] = 0;
	fun(a,num+1,grade-num);
	a[num] = 1;
	fun(a,num+1,grade*2);	
}
int main()
{
	int a[11];
	fun(a,1,10);
	return 0;
}
